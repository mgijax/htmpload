#!/bin/csh -f

#
# config variables for Sanger/Annotation load
#
# for appending additional MP/OMIM annotations
#

setenv ANNOTDATADIR		${OUTPUTDIR}
setenv ANNOTMODE		append
setenv ANNOTTYPENAME		"Mammalian Phenotype/Genotype"
setenv ANNOTPROPERTY   		86
setenv ANNOTINPUTFILE		${ANNOT_INPUT_FILE}
setenv ANNOTLOG			${LOGDIR}/htmp_annot.log
# delete data by user, not by reference
setenv DELETEREFERENCE		J:0
setenv DELETEUSER		scrum-dog
setenv ANNOTOBSOLETE		0
setenv ANNOTTRANSMISSION        1

